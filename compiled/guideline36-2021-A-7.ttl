@prefix ns1: <http://data.ashrae.org/standard223#> .
@prefix ns2: <http://qudt.org/schema/qudt/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<urn:ex/> a owl:Ontology .

<urn:ex/cold-deck-damper-actuator> a ns1:Actuator ;
    ns1:actuates <urn:ex/cold-deck-damper> ;
    ns1:commandedByProperty <urn:ex/cold-deck-damper-command> .

<urn:ex/cold-deck-supply-air-flow-sensor> a ns1:Sensor ;
    ns1:hasObservationLocation <urn:ex/cold-deck-in> ;
    ns1:observes <urn:ex/cold-deck-supply-air-flow> .

<urn:ex/dual-duct-terminal-unit-with-inlet-sensors> a ns1:DualDuctTerminal,
        <http://data.ashrae.org/standard223/1.0/extensions/g36#DualDuctTerminalWithInletSensors> ;
    ns1:cnx <urn:ex/cold-deck-in>,
        <urn:ex/hot-deck-in>,
        <urn:ex/out> ;
    ns1:contains <urn:ex/cold-deck-damper>,
        <urn:ex/hot-deck-damper>,
        <urn:ex/junction> .

<urn:ex/hot-deck-damper-actuator> a ns1:Actuator ;
    ns1:actuates <urn:ex/hot-deck-damper> ;
    ns1:commandedByProperty <urn:ex/hot-deck-damper-command> .

<urn:ex/hot-deck-supply-air-flow-sensor> a ns1:Sensor ;
    ns1:hasObservationLocation <urn:ex/hot-deck-in> ;
    ns1:observes <urn:ex/hot-deck-supply-air-flow> .

<urn:ex/cold-deck-damper-in> a ns1:InletConnectionPoint ;
    ns1:cnx <urn:ex/duct-before-cold-deck-damper> ;
    ns1:hasMedium ns1:Medium-Air ;
    ns1:mapsTo <urn:ex/cold-deck-in> .

<urn:ex/cold-deck-damper-out> a ns1:OutletConnectionPoint ;
    ns1:cnx <urn:ex/duct-after-cold-deck-damper> ;
    ns1:hasMedium ns1:Medium-Air .

<urn:ex/cold-deck-damper-position> a ns1:QuantifiableObservableProperty ;
    ns2:hasQuantityKind <http://qudt.org/vocab/quantitykind/DimensionlessRatio> ;
    ns2:hasUnit <http://qudt.org/vocab/unit/PERCENT> .

<urn:ex/cold-deck-in-connection> a ns1:Connection ;
    ns1:hasMedium ns1:Medium-Air .

<urn:ex/duct-before-cold-deck-damper> a ns1:Connection,
        ns1:Duct ;
    ns1:hasMedium ns1:Medium-Air .

<urn:ex/duct-before-hot-deck-damper> a ns1:Connection,
        ns1:Duct ;
    ns1:hasMedium ns1:Medium-Air .

<urn:ex/hot-deck-damper-in> a ns1:InletConnectionPoint ;
    ns1:cnx <urn:ex/duct-before-hot-deck-damper> ;
    ns1:hasMedium ns1:Medium-Air ;
    ns1:mapsTo <urn:ex/hot-deck-in> .

<urn:ex/hot-deck-damper-out> a ns1:OutletConnectionPoint ;
    ns1:cnx <urn:ex/duct-after-hot-deck-damper> ;
    ns1:hasMedium ns1:Medium-Air .

<urn:ex/hot-deck-damper-position> a ns1:QuantifiableObservableProperty ;
    ns2:hasQuantityKind <http://qudt.org/vocab/quantitykind/DimensionlessRatio> ;
    ns2:hasUnit <http://qudt.org/vocab/unit/PERCENT> .

<urn:ex/hot-deck-in-connection> a ns1:Connection ;
    ns1:hasMedium ns1:Medium-Air .

<urn:ex/junction> a ns1:Junction ;
    ns1:cnx <urn:ex/junction-in-1>,
        <urn:ex/junction-in-2>,
        <urn:ex/junction-out> ;
    ns1:hasMedium ns1:Medium-Air .

<urn:ex/junction-in-1> a ns1:InletConnectionPoint ;
    ns1:cnx <urn:ex/duct-after-hot-deck-damper> ;
    ns1:hasMedium ns1:Medium-Air .

<urn:ex/junction-in-2> a ns1:InletConnectionPoint ;
    ns1:cnx <urn:ex/duct-after-cold-deck-damper> ;
    ns1:hasMedium ns1:Medium-Air .

<urn:ex/junction-out> a ns1:OutletConnectionPoint ;
    ns1:cnx <urn:ex/out-connection> ;
    ns1:hasMedium ns1:Medium-Air ;
    ns1:mapsTo <urn:ex/out> .

<urn:ex/cold-deck-damper> a ns1:Damper,
        <http://data.ashrae.org/standard223/1.0/extensions/g36#Damper> ;
    ns1:cnx <urn:ex/cold-deck-damper-in>,
        <urn:ex/cold-deck-damper-out> ;
    ns1:hasProperty <urn:ex/cold-deck-damper-command>,
        <urn:ex/cold-deck-damper-position> ;
    ns1:hasRole ns1:Role-Cooling,
        ns1:Role-Supply .

<urn:ex/cold-deck-damper-command> a ns1:QuantifiableActuatableProperty ;
    ns1:hasAspect ns1:EnumerationKind-Position ;
    ns2:hasQuantityKind <http://qudt.org/vocab/quantitykind/DimensionlessRatio> ;
    ns2:hasUnit <http://qudt.org/vocab/unit/PERCENT> .

<urn:ex/cold-deck-supply-air-flow> a ns1:QuantifiableObservableProperty ;
    ns1:hasRole ns1:Role-Cooling,
        ns1:Role-Supply ;
    ns2:hasQuantityKind <http://qudt.org/vocab/quantitykind/VolumeFlowRate> ;
    ns2:hasUnit <http://qudt.org/vocab/unit/FT3-PER-MIN> .

<urn:ex/duct-after-cold-deck-damper> a ns1:Connection,
        ns1:Duct ;
    ns1:hasMedium ns1:Medium-Air .

<urn:ex/duct-after-hot-deck-damper> a ns1:Connection,
        ns1:Duct ;
    ns1:hasMedium ns1:Medium-Air .

<urn:ex/hot-deck-damper> a ns1:Damper,
        <http://data.ashrae.org/standard223/1.0/extensions/g36#Damper> ;
    ns1:cnx <urn:ex/hot-deck-damper-in>,
        <urn:ex/hot-deck-damper-out> ;
    ns1:hasProperty <urn:ex/hot-deck-damper-command>,
        <urn:ex/hot-deck-damper-position> ;
    ns1:hasRole ns1:Role-Heating,
        ns1:Role-Supply .

<urn:ex/hot-deck-damper-command> a ns1:QuantifiableActuatableProperty ;
    ns1:hasAspect ns1:EnumerationKind-Position ;
    ns2:hasQuantityKind <http://qudt.org/vocab/quantitykind/DimensionlessRatio> ;
    ns2:hasUnit <http://qudt.org/vocab/unit/PERCENT> .

<urn:ex/hot-deck-supply-air-flow> a ns1:QuantifiableObservableProperty ;
    ns1:hasRole ns1:Role-Heating,
        ns1:Role-Supply ;
    ns2:hasQuantityKind <http://qudt.org/vocab/quantitykind/VolumeFlowRate> ;
    ns2:hasUnit <http://qudt.org/vocab/unit/FT3-PER-MIN> .

<urn:ex/out> a ns1:OutletConnectionPoint ;
    ns1:cnx <urn:ex/out-connection> ;
    ns1:hasMedium ns1:Medium-Air .

<urn:ex/out-connection> a ns1:Connection ;
    ns1:hasMedium ns1:Medium-Air .

<urn:ex/cold-deck-in> a ns1:InletConnectionPoint ;
    ns1:cnx <urn:ex/cold-deck-in-connection> ;
    ns1:hasMedium ns1:Medium-Air ;
    ns1:hasProperty <urn:ex/cold-deck-supply-air-flow> .

<urn:ex/hot-deck-in> a ns1:InletConnectionPoint ;
    ns1:cnx <urn:ex/hot-deck-in-connection> ;
    ns1:hasMedium ns1:Medium-Air ;
    ns1:hasProperty <urn:ex/hot-deck-supply-air-flow> .

