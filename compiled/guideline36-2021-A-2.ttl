@prefix ns1: <http://qudt.org/schema/qudt/> .
@prefix ns2: <http://data.ashrae.org/standard223#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<urn:ex/> a owl:Ontology .

<urn:ex/damper-actuator> a ns2:Actuator ;
    rdfs:label "damper actuator" ;
    ns2:actuates <urn:ex/damper> ;
    ns2:commandedByProperty <urn:ex/damper-command> .

<urn:ex/discharge-air-flow-sensor> a ns2:Sensor ;
    rdfs:label "discharge air flow sensor" ;
    ns2:hasObservationLocation <urn:ex/air-in> ;
    ns2:observes <urn:ex/discharge-air-flow> .

<urn:ex/discharge-air-temp-sensor> a ns2:Sensor ;
    rdfs:label "discharge air temp sensor" ;
    ns2:hasObservationLocation <urn:ex/air-out> ;
    ns2:observes <urn:ex/discharge-air-temp> .

<urn:ex/reheat-coil-valve-actuator> a ns2:Actuator ;
    rdfs:label "reheat coil valve actuator" ;
    ns2:actuates <urn:ex/reheat-coil-valve> ;
    ns2:commandedByProperty <urn:ex/reheat-coil-valve-command> .

<urn:ex/damper-position> a ns2:QuantifiableObservableProperty ;
    rdfs:label "damper position" ;
    ns1:hasQuantityKind <http://qudt.org/vocab/quantitykind/DimensionlessRatio> ;
    ns1:hasUnit <http://qudt.org/vocab/unit/PERCENT> .

<urn:ex/reheat-coil-valve-position> a ns2:QuantifiableObservableProperty ;
    rdfs:label "reheat coil valve position" ;
    ns1:hasQuantityKind <http://qudt.org/vocab/quantitykind/DimensionlessRatio> ;
    ns1:hasUnit <http://qudt.org/vocab/unit/PERCENT> .

<urn:ex/damper-command> a ns2:QuantifiableActuatableProperty ;
    rdfs:label "damper command" ;
    ns2:hasAspect ns2:EnumerationKind-Position ;
    ns1:hasQuantityKind <http://qudt.org/vocab/quantitykind/DimensionlessRatio> ;
    ns1:hasUnit <http://qudt.org/vocab/unit/PERCENT> .

<urn:ex/discharge-air-temp> a ns2:QuantifiableObservableProperty ;
    rdfs:label "discharge air temp" ;
    ns2:hasAspect ns2:Role-Discharge ;
    ns1:hasQuantityKind <http://qudt.org/vocab/quantitykind/Temperature> ;
    ns1:hasUnit <http://qudt.org/vocab/unit/DEG_C> .

<urn:ex/reheat-coil-valve-command> a ns2:QuantifiableActuatableProperty ;
    rdfs:label "reheat coil valve command" ;
    ns2:hasAspect ns2:EnumerationKind-Position ;
    ns1:hasQuantityKind <http://qudt.org/vocab/quantitykind/DimensionlessRatio> ;
    ns1:hasUnit <http://qudt.org/vocab/unit/PERCENT> .

<urn:ex/discharge-air-flow> a ns2:QuantifiableObservableProperty ;
    rdfs:label "discharge air flow" ;
    ns2:hasAspect ns2:Role-Discharge ;
    ns1:hasQuantityKind <http://qudt.org/vocab/quantitykind/VolumeFlowRate> ;
    ns1:hasUnit <http://qudt.org/vocab/unit/FT3-PER-MIN> .

<urn:ex/air-in-connection> a ns2:Connection,
        ns2:Duct ;
    rdfs:label "air in connection" ;
    ns2:cnx <urn:ex/air-in>,
        <urn:ex/damper-in> ;
    ns2:connectsAt <urn:ex/air-in>,
        <urn:ex/damper-in> ;
    ns2:connectsTo <urn:ex/damper>,
        <urn:ex/vav-reheat> ;
    ns2:hasMedium ns2:Medium-Air .

<urn:ex/air-out-connection> a ns2:Connection,
        ns2:Duct ;
    rdfs:label "air out connection" ;
    ns2:cnx <urn:ex/air-out>,
        <urn:ex/reheat-coil-air-out> ;
    ns2:connectsAt <urn:ex/air-out>,
        <urn:ex/reheat-coil-air-out> ;
    ns2:connectsFrom <urn:ex/reheat-coil>,
        <urn:ex/vav-reheat> ;
    ns2:hasMedium ns2:Medium-Air .

<urn:ex/damper-in> a ns2:InletConnectionPoint ;
    rdfs:label "damper in" ;
    ns2:cnx <urn:ex/air-in-connection>,
        <urn:ex/damper> ;
    ns2:connectsThrough <urn:ex/air-in-connection> ;
    ns2:hasMedium ns2:Medium-Air ;
    ns2:isConnectionPointOf <urn:ex/damper> ;
    ns2:mapsTo <urn:ex/air-in> .

<urn:ex/damper-out> a ns2:OutletConnectionPoint ;
    rdfs:label "damper out" ;
    ns2:cnx <urn:ex/damper>,
        <urn:ex/duct-after-damper> ;
    ns2:connectsThrough <urn:ex/duct-after-damper> ;
    ns2:hasMedium ns2:Medium-Air ;
    ns2:isConnectionPointOf <urn:ex/damper> .

<urn:ex/duct-after-damper> a ns2:Connection,
        ns2:Duct ;
    rdfs:label "duct after damper" ;
    ns2:cnx <urn:ex/damper-out>,
        <urn:ex/reheat-coil-air-in> ;
    ns2:connectsAt <urn:ex/damper-out>,
        <urn:ex/reheat-coil-air-in> ;
    ns2:connectsFrom <urn:ex/damper> ;
    ns2:connectsTo <urn:ex/reheat-coil> ;
    ns2:hasMedium ns2:Medium-Air .

<urn:ex/hot-water-in-connection> a ns2:Connection,
        ns2:Pipe ;
    rdfs:label "hot water in connection" ;
    ns2:cnx <urn:ex/hot-water-in>,
        <urn:ex/reheat-coil-water-in> ;
    ns2:connectsAt <urn:ex/hot-water-in>,
        <urn:ex/reheat-coil-water-in> ;
    ns2:connectsTo <urn:ex/reheat-coil>,
        <urn:ex/vav-reheat> ;
    ns2:hasMedium ns2:Medium-Water .

<urn:ex/hot-water-out-connection> a ns2:Connection,
        ns2:Pipe ;
    rdfs:label "hot water out connection" ;
    ns2:cnx <urn:ex/hot-water-out>,
        <urn:ex/reheat-coil-valve-out> ;
    ns2:connectsAt <urn:ex/hot-water-out>,
        <urn:ex/reheat-coil-valve-out> ;
    ns2:connectsFrom <urn:ex/reheat-coil-valve>,
        <urn:ex/vav-reheat> ;
    ns2:hasMedium ns2:Medium-Water .

<urn:ex/pipe-hot-water-return-before-valve> a ns2:Connection,
        ns2:Pipe ;
    rdfs:label "pipe hot water return before valve" ;
    ns2:cnx <urn:ex/reheat-coil-valve-in>,
        <urn:ex/reheat-coil-water-out> ;
    ns2:connectsAt <urn:ex/reheat-coil-valve-in>,
        <urn:ex/reheat-coil-water-out> ;
    ns2:connectsFrom <urn:ex/reheat-coil> ;
    ns2:connectsTo <urn:ex/reheat-coil-valve> ;
    ns2:hasMedium ns2:Medium-Water .

<urn:ex/reheat-coil-air-in> a ns2:InletConnectionPoint ;
    rdfs:label "reheat coil air in" ;
    ns2:cnx <urn:ex/duct-after-damper>,
        <urn:ex/reheat-coil> ;
    ns2:connectsThrough <urn:ex/duct-after-damper> ;
    ns2:hasMedium ns2:Medium-Air ;
    ns2:isConnectionPointOf <urn:ex/reheat-coil> .

<urn:ex/reheat-coil-air-out> a ns2:OutletConnectionPoint ;
    rdfs:label "reheat coil air out" ;
    ns2:cnx <urn:ex/air-out-connection>,
        <urn:ex/reheat-coil> ;
    ns2:connectsThrough <urn:ex/air-out-connection> ;
    ns2:hasMedium ns2:Medium-Air ;
    ns2:isConnectionPointOf <urn:ex/reheat-coil> ;
    ns2:mapsTo <urn:ex/air-out> .

<urn:ex/reheat-coil-valve-in> a ns2:InletConnectionPoint ;
    rdfs:label "reheat coil valve in" ;
    ns2:cnx <urn:ex/pipe-hot-water-return-before-valve>,
        <urn:ex/reheat-coil-valve> ;
    ns2:connectsThrough <urn:ex/pipe-hot-water-return-before-valve> ;
    ns2:hasMedium ns2:Medium-Water ;
    ns2:isConnectionPointOf <urn:ex/reheat-coil-valve> .

<urn:ex/reheat-coil-valve-out> a ns2:OutletConnectionPoint ;
    rdfs:label "reheat coil valve out" ;
    ns2:cnx <urn:ex/hot-water-out-connection>,
        <urn:ex/reheat-coil-valve> ;
    ns2:connectsThrough <urn:ex/hot-water-out-connection> ;
    ns2:hasMedium ns2:Medium-Water ;
    ns2:isConnectionPointOf <urn:ex/reheat-coil-valve> ;
    ns2:mapsTo <urn:ex/hot-water-out> .

<urn:ex/reheat-coil-water-in> a ns2:InletConnectionPoint ;
    rdfs:label "reheat coil water in" ;
    ns2:cnx <urn:ex/hot-water-in-connection>,
        <urn:ex/reheat-coil> ;
    ns2:connectsThrough <urn:ex/hot-water-in-connection> ;
    ns2:hasMedium ns2:Medium-Water ;
    ns2:isConnectionPointOf <urn:ex/reheat-coil> ;
    ns2:mapsTo <urn:ex/hot-water-in> .

<urn:ex/reheat-coil-water-out> a ns2:OutletConnectionPoint ;
    rdfs:label "reheat coil water out" ;
    ns2:cnx <urn:ex/pipe-hot-water-return-before-valve>,
        <urn:ex/reheat-coil> ;
    ns2:connectsThrough <urn:ex/pipe-hot-water-return-before-valve> ;
    ns2:hasMedium ns2:Medium-Water ;
    ns2:isConnectionPointOf <urn:ex/reheat-coil> .

<urn:ex/hot-water-in> a ns2:InletConnectionPoint ;
    rdfs:label "hot water in" ;
    ns2:cnx <urn:ex/hot-water-in-connection>,
        <urn:ex/vav-reheat> ;
    ns2:connectsThrough <urn:ex/hot-water-in-connection> ;
    ns2:hasMedium ns2:Medium-Water ;
    ns2:isConnectionPointOf <urn:ex/vav-reheat> .

<urn:ex/hot-water-out> a ns2:OutletConnectionPoint ;
    rdfs:label "hot water out" ;
    ns2:cnx <urn:ex/hot-water-out-connection>,
        <urn:ex/vav-reheat> ;
    ns2:connectsThrough <urn:ex/hot-water-out-connection> ;
    ns2:hasMedium ns2:Medium-Water ;
    ns2:isConnectionPointOf <urn:ex/vav-reheat> .

<urn:ex/air-in> a ns2:InletConnectionPoint ;
    rdfs:label "air in" ;
    ns2:cnx <urn:ex/air-in-connection>,
        <urn:ex/vav-reheat> ;
    ns2:connectsThrough <urn:ex/air-in-connection> ;
    ns2:hasMedium ns2:Medium-Air ;
    ns2:hasProperty <urn:ex/discharge-air-flow> ;
    ns2:isConnectionPointOf <urn:ex/vav-reheat> .

<urn:ex/air-out> a ns2:OutletConnectionPoint ;
    rdfs:label "air out" ;
    ns2:cnx <urn:ex/air-out-connection>,
        <urn:ex/vav-reheat> ;
    ns2:connectsThrough <urn:ex/air-out-connection> ;
    ns2:hasMedium ns2:Medium-Air ;
    ns2:hasProperty <urn:ex/discharge-air-flow>,
        <urn:ex/discharge-air-temp> ;
    ns2:isConnectionPointOf <urn:ex/vav-reheat> .

<urn:ex/damper> a ns2:Damper,
        <http://data.ashrae.org/standard223/1.0/extensions/g36#Damper> ;
    rdfs:label "damper" ;
    ns2:cnx <urn:ex/damper-in>,
        <urn:ex/damper-out> ;
    ns2:hasConnectionPoint <urn:ex/damper-in>,
        <urn:ex/damper-out> ;
    ns2:hasProperty <urn:ex/damper-command>,
        <urn:ex/damper-position> .

<urn:ex/reheat-coil-valve> a ns2:Valve,
        <http://data.ashrae.org/standard223/1.0/extensions/g36#ChilledWaterValve>,
        <http://data.ashrae.org/standard223/1.0/extensions/g36#HotWaterValve> ;
    rdfs:label "reheat coil valve" ;
    ns2:cnx <urn:ex/reheat-coil-valve-in>,
        <urn:ex/reheat-coil-valve-out> ;
    ns2:hasConnectionPoint <urn:ex/reheat-coil-valve-in>,
        <urn:ex/reheat-coil-valve-out> ;
    ns2:hasMedium ns2:Water-HotWater ;
    ns2:hasProperty <urn:ex/reheat-coil-valve-command>,
        <urn:ex/reheat-coil-valve-position> .

<urn:ex/vav-reheat> a ns2:SingleDuctTerminal,
        <http://data.ashrae.org/standard223/1.0/extensions/g36#VAVWithReheat> ;
    rdfs:label "vav reheat" ;
    ns2:cnx <urn:ex/air-in>,
        <urn:ex/air-out>,
        <urn:ex/hot-water-in>,
        <urn:ex/hot-water-out> ;
    ns2:contains <urn:ex/damper>,
        <urn:ex/reheat-coil>,
        <urn:ex/reheat-coil-valve> ;
    ns2:hasConnectionPoint <urn:ex/air-in>,
        <urn:ex/air-out>,
        <urn:ex/hot-water-in>,
        <urn:ex/hot-water-out> .

<urn:ex/reheat-coil> a ns2:HeatingCoil,
        <http://data.ashrae.org/standard223/1.0/extensions/g36#HotWaterCoil> ;
    rdfs:label "reheat coil" ;
    ns2:cnx <urn:ex/reheat-coil-air-in>,
        <urn:ex/reheat-coil-air-out>,
        <urn:ex/reheat-coil-water-in>,
        <urn:ex/reheat-coil-water-out> ;
    ns2:hasConnectionPoint <urn:ex/reheat-coil-air-in>,
        <urn:ex/reheat-coil-air-out>,
        <urn:ex/reheat-coil-water-in>,
        <urn:ex/reheat-coil-water-out> ;
    ns2:hasRole ns2:Role-Heating .

