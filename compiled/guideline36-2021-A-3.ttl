@prefix ns1: <http://data.ashrae.org/standard223#> .
@prefix ns2: <http://qudt.org/schema/qudt/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<urn:ex/> a owl:Ontology .

<urn:ex/damper-actuator> a ns1:Actuator ;
    rdfs:label "damper actuator" ;
    ns1:actuates <urn:ex/damper> ;
    ns1:commandedByProperty <urn:ex/damper-command> .

<urn:ex/discharge-air-temp-sensor> a ns1:Sensor ;
    rdfs:label "discharge air temp sensor" ;
    ns1:hasObservationLocation <urn:ex/out> ;
    ns1:observes <urn:ex/discharge-air-temp> .

<urn:ex/reheat-coil-valve-actuator> a ns1:Actuator ;
    rdfs:label "reheat coil valve actuator" ;
    ns1:actuates <urn:ex/reheat-coil-valve> ;
    ns1:commandedByProperty <urn:ex/reheat-coil-valve-command> .

<urn:ex/supply-air-flow-sensor> a ns1:Sensor ;
    rdfs:label "supply air flow sensor" ;
    ns1:hasObservationLocation <urn:ex/supply-air-in> ;
    ns1:observes <urn:ex/supply-air-flow> .

<urn:ex/damper-position> a ns1:QuantifiableObservableProperty ;
    rdfs:label "damper position" ;
    ns2:hasQuantityKind <http://qudt.org/vocab/quantitykind/DimensionlessRatio> ;
    ns2:hasUnit <http://qudt.org/vocab/unit/PERCENT> .

<urn:ex/fan-start-cmd> a ns1:EnumeratedActuatableProperty ;
    rdfs:label "fan start cmd" ;
    ns1:hasEnumerationKind ns1:EnumerationKind-OnOff .

<urn:ex/fan-start-status> a ns1:EnumeratedObservableProperty ;
    rdfs:label "fan start status" ;
    ns1:hasEnumerationKind ns1:EnumerationKind-OnOff .

<urn:ex/reheat-coil-valve-position> a ns1:QuantifiableObservableProperty ;
    rdfs:label "reheat coil valve position" ;
    ns2:hasQuantityKind <http://qudt.org/vocab/quantitykind/DimensionlessRatio> ;
    ns2:hasUnit <http://qudt.org/vocab/unit/PERCENT> .

<urn:ex/damper-command> a ns1:QuantifiableActuatableProperty ;
    rdfs:label "damper command" ;
    ns1:hasAspect ns1:EnumerationKind-Position ;
    ns2:hasQuantityKind <http://qudt.org/vocab/quantitykind/DimensionlessRatio> ;
    ns2:hasUnit <http://qudt.org/vocab/unit/PERCENT> .

<urn:ex/discharge-air-temp> a ns1:QuantifiableObservableProperty ;
    rdfs:label "discharge air temp" ;
    ns1:hasRole ns1:Role-Discharge ;
    ns2:hasQuantityKind <http://qudt.org/vocab/quantitykind/Temperature> ;
    ns2:hasUnit <http://qudt.org/vocab/unit/DEG_C> .

<urn:ex/duct-before-damper> a ns1:Connection,
        ns1:Duct ;
    rdfs:label "duct before damper" ;
    ns1:cnx <urn:ex/damper-in> ;
    ns1:connectsAt <urn:ex/damper-in> ;
    ns1:connectsTo <urn:ex/damper> ;
    ns1:hasMedium ns1:Fluid-Air .

<urn:ex/duct-before-reheat-coil> a ns1:Connection,
        ns1:Duct ;
    rdfs:label "duct before reheat coil" ;
    ns1:cnx <urn:ex/reheat-coil-in> ;
    ns1:connectsAt <urn:ex/reheat-coil-in> ;
    ns1:connectsTo <urn:ex/reheat-coil> ;
    ns1:hasMedium ns1:Fluid-Air .

<urn:ex/pipe-hot-water-return-before-valve> a ns1:Connection,
        ns1:Pipe ;
    rdfs:label "pipe hot water return before valve" ;
    ns1:cnx <urn:ex/reheat-coil-valve-in> ;
    ns1:connectsAt <urn:ex/reheat-coil-valve-in> ;
    ns1:connectsTo <urn:ex/reheat-coil-valve> ;
    ns1:hasMedium ns1:Medium-Water .

<urn:ex/pipe-hot-water-supply> a ns1:Connection,
        ns1:Pipe ;
    rdfs:label "pipe hot water supply" ;
    ns1:cnx <urn:ex/reheat-coil-water-out> ;
    ns1:connectsAt <urn:ex/reheat-coil-water-out> ;
    ns1:connectsFrom <urn:ex/reheat-coil> ;
    ns1:hasMedium ns1:Medium-Water .

<urn:ex/reheat-coil-valve-command> a ns1:QuantifiableActuatableProperty ;
    rdfs:label "reheat coil valve command" ;
    ns1:hasAspect ns1:EnumerationKind-Position ;
    ns2:hasQuantityKind <http://qudt.org/vocab/quantitykind/DimensionlessRatio> ;
    ns2:hasUnit <http://qudt.org/vocab/unit/PERCENT> .

<urn:ex/return-air-from-plenum-in-connection> a ns1:Connection ;
    rdfs:label "return air from plenum in connection" ;
    ns1:cnx <urn:ex/return-air-from-plenum-in> ;
    ns1:connectsAt <urn:ex/return-air-from-plenum-in> ;
    ns1:connectsTo <urn:ex/parallel-fan-powered-terminal-unit-constant-volume-fan> ;
    ns1:hasMedium ns1:Fluid-Air .

<urn:ex/supply-air-flow> a ns1:QuantifiableObservableProperty ;
    rdfs:label "supply air flow" ;
    ns1:hasRole ns1:Role-Supply ;
    ns2:hasQuantityKind <http://qudt.org/vocab/quantitykind/VolumeFlowRate> ;
    ns2:hasUnit <http://qudt.org/vocab/unit/FT3-PER-MIN> .

<urn:ex/supply-air-in-connection> a ns1:Connection ;
    rdfs:label "supply air in connection" ;
    ns1:cnx <urn:ex/supply-air-in> ;
    ns1:connectsAt <urn:ex/supply-air-in> ;
    ns1:connectsTo <urn:ex/parallel-fan-powered-terminal-unit-constant-volume-fan> ;
    ns1:hasMedium ns1:Fluid-Air .

<urn:ex/damper-in> a ns1:InletConnectionPoint ;
    rdfs:label "damper in" ;
    ns1:cnx <urn:ex/damper>,
        <urn:ex/duct-before-damper> ;
    ns1:connectsThrough <urn:ex/duct-before-damper> ;
    ns1:hasMedium ns1:Fluid-Air ;
    ns1:isConnectionPointOf <urn:ex/damper> ;
    ns1:mapsTo <urn:ex/supply-air-in> .

<urn:ex/damper-out> a ns1:OutletConnectionPoint ;
    rdfs:label "damper out" ;
    ns1:cnx <urn:ex/damper>,
        <urn:ex/duct-after-damper> ;
    ns1:connectsThrough <urn:ex/duct-after-damper> ;
    ns1:hasMedium ns1:Fluid-Air ;
    ns1:isConnectionPointOf <urn:ex/damper> .

<urn:ex/duct-after-damper> a ns1:Connection,
        ns1:Duct ;
    rdfs:label "duct after damper" ;
    ns1:cnx <urn:ex/damper-out>,
        <urn:ex/junction-in-1> ;
    ns1:connectsAt <urn:ex/damper-out>,
        <urn:ex/junction-in-1> ;
    ns1:connectsFrom <urn:ex/damper> ;
    ns1:connectsTo <urn:ex/junction> ;
    ns1:hasMedium ns1:Fluid-Air .

<urn:ex/duct-after-fan> a ns1:Connection,
        ns1:Duct ;
    rdfs:label "duct after fan" ;
    ns1:cnx <urn:ex/fan-out>,
        <urn:ex/junction-in-2> ;
    ns1:connectsAt <urn:ex/fan-out>,
        <urn:ex/junction-in-2> ;
    ns1:connectsFrom <urn:ex/fan> ;
    ns1:connectsTo <urn:ex/junction> ;
    ns1:hasMedium ns1:Fluid-Air .

<urn:ex/duct-after-reheat-coil> a ns1:Connection,
        ns1:Duct ;
    rdfs:label "duct after reheat coil" ;
    ns1:cnx <urn:ex/fan-in>,
        <urn:ex/reheat-coil-air-out> ;
    ns1:connectsAt <urn:ex/fan-in>,
        <urn:ex/reheat-coil-air-out> ;
    ns1:connectsFrom <urn:ex/reheat-coil> ;
    ns1:connectsTo <urn:ex/fan> ;
    ns1:hasMedium ns1:Fluid-Air .

<urn:ex/fan-in> a ns1:InletConnectionPoint ;
    rdfs:label "fan in" ;
    ns1:cnx <urn:ex/duct-after-reheat-coil>,
        <urn:ex/fan> ;
    ns1:connectsThrough <urn:ex/duct-after-reheat-coil> ;
    ns1:hasMedium ns1:Fluid-Air ;
    ns1:isConnectionPointOf <urn:ex/fan> .

<urn:ex/fan-out> a ns1:OutletConnectionPoint ;
    rdfs:label "fan out" ;
    ns1:cnx <urn:ex/duct-after-fan>,
        <urn:ex/fan> ;
    ns1:connectsThrough <urn:ex/duct-after-fan> ;
    ns1:hasMedium ns1:Fluid-Air ;
    ns1:isConnectionPointOf <urn:ex/fan> .

<urn:ex/junction-in-1> a ns1:InletConnectionPoint ;
    rdfs:label "junction in 1" ;
    ns1:cnx <urn:ex/duct-after-damper>,
        <urn:ex/junction> ;
    ns1:connectsThrough <urn:ex/duct-after-damper> ;
    ns1:hasMedium ns1:Fluid-Air ;
    ns1:isConnectionPointOf <urn:ex/junction> .

<urn:ex/junction-in-2> a ns1:InletConnectionPoint ;
    rdfs:label "junction in 2" ;
    ns1:cnx <urn:ex/duct-after-fan>,
        <urn:ex/junction> ;
    ns1:connectsThrough <urn:ex/duct-after-fan> ;
    ns1:hasMedium ns1:Fluid-Air ;
    ns1:isConnectionPointOf <urn:ex/junction> .

<urn:ex/junction-out> a ns1:OutletConnectionPoint ;
    rdfs:label "junction out" ;
    ns1:cnx <urn:ex/junction>,
        <urn:ex/out-connection> ;
    ns1:connectsThrough <urn:ex/out-connection> ;
    ns1:hasMedium ns1:Fluid-Air ;
    ns1:isConnectionPointOf <urn:ex/junction> ;
    ns1:mapsTo <urn:ex/out> .

<urn:ex/out-connection> a ns1:Connection ;
    rdfs:label "out connection" ;
    ns1:cnx <urn:ex/junction-out>,
        <urn:ex/out> ;
    ns1:connectsAt <urn:ex/junction-out>,
        <urn:ex/out> ;
    ns1:connectsFrom <urn:ex/junction>,
        <urn:ex/parallel-fan-powered-terminal-unit-constant-volume-fan> ;
    ns1:hasMedium ns1:Fluid-Air .

<urn:ex/pipe-hot-water-return-after-valve> a ns1:Connection,
        ns1:Pipe ;
    rdfs:label "pipe hot water return after valve" ;
    ns1:cnx <urn:ex/reheat-coil-valve-out>,
        <urn:ex/reheat-coil-water-in> ;
    ns1:connectsAt <urn:ex/reheat-coil-valve-out>,
        <urn:ex/reheat-coil-water-in> ;
    ns1:connectsFrom <urn:ex/reheat-coil-valve> ;
    ns1:connectsTo <urn:ex/reheat-coil> ;
    ns1:hasMedium ns1:Medium-Water .

<urn:ex/reheat-coil-air-out> a ns1:OutletConnectionPoint ;
    rdfs:label "reheat coil air out" ;
    ns1:cnx <urn:ex/duct-after-reheat-coil>,
        <urn:ex/reheat-coil> ;
    ns1:connectsThrough <urn:ex/duct-after-reheat-coil> ;
    ns1:hasMedium ns1:Fluid-Air ;
    ns1:isConnectionPointOf <urn:ex/reheat-coil> .

<urn:ex/reheat-coil-in> a ns1:InletConnectionPoint ;
    rdfs:label "reheat coil in" ;
    ns1:cnx <urn:ex/duct-before-reheat-coil>,
        <urn:ex/reheat-coil> ;
    ns1:connectsThrough <urn:ex/duct-before-reheat-coil> ;
    ns1:hasMedium ns1:Fluid-Air ;
    ns1:isConnectionPointOf <urn:ex/reheat-coil> ;
    ns1:mapsTo <urn:ex/return-air-from-plenum-in> .

<urn:ex/reheat-coil-valve-in> a ns1:InletConnectionPoint ;
    rdfs:label "reheat coil valve in" ;
    ns1:cnx <urn:ex/pipe-hot-water-return-before-valve>,
        <urn:ex/reheat-coil-valve> ;
    ns1:connectsThrough <urn:ex/pipe-hot-water-return-before-valve> ;
    ns1:hasMedium ns1:Medium-Water ;
    ns1:isConnectionPointOf <urn:ex/reheat-coil-valve> .

<urn:ex/reheat-coil-valve-out> a ns1:OutletConnectionPoint ;
    rdfs:label "reheat coil valve out" ;
    ns1:cnx <urn:ex/pipe-hot-water-return-after-valve>,
        <urn:ex/reheat-coil-valve> ;
    ns1:connectsThrough <urn:ex/pipe-hot-water-return-after-valve> ;
    ns1:hasMedium ns1:Medium-Water ;
    ns1:isConnectionPointOf <urn:ex/reheat-coil-valve> .

<urn:ex/reheat-coil-water-in> a ns1:InletConnectionPoint ;
    rdfs:label "reheat coil water in" ;
    ns1:cnx <urn:ex/pipe-hot-water-return-after-valve>,
        <urn:ex/reheat-coil> ;
    ns1:connectsThrough <urn:ex/pipe-hot-water-return-after-valve> ;
    ns1:hasMedium ns1:Medium-Water ;
    ns1:isConnectionPointOf <urn:ex/reheat-coil> .

<urn:ex/reheat-coil-water-out> a ns1:OutletConnectionPoint ;
    rdfs:label "reheat coil water out" ;
    ns1:cnx <urn:ex/pipe-hot-water-supply>,
        <urn:ex/reheat-coil> ;
    ns1:connectsThrough <urn:ex/pipe-hot-water-supply> ;
    ns1:hasMedium ns1:Medium-Water ;
    ns1:isConnectionPointOf <urn:ex/reheat-coil> .

<urn:ex/return-air-from-plenum-in> a ns1:InletConnectionPoint ;
    rdfs:label "return air from plenum in" ;
    ns1:cnx <urn:ex/parallel-fan-powered-terminal-unit-constant-volume-fan>,
        <urn:ex/return-air-from-plenum-in-connection> ;
    ns1:connectsThrough <urn:ex/return-air-from-plenum-in-connection> ;
    ns1:hasMedium ns1:Fluid-Air ;
    ns1:isConnectionPointOf <urn:ex/parallel-fan-powered-terminal-unit-constant-volume-fan> .

<urn:ex/out> a ns1:OutletConnectionPoint ;
    rdfs:label "out" ;
    ns1:cnx <urn:ex/out-connection>,
        <urn:ex/parallel-fan-powered-terminal-unit-constant-volume-fan> ;
    ns1:connectsThrough <urn:ex/out-connection> ;
    ns1:hasMedium ns1:Fluid-Air ;
    ns1:hasProperty <urn:ex/discharge-air-temp> ;
    ns1:isConnectionPointOf <urn:ex/parallel-fan-powered-terminal-unit-constant-volume-fan> .

<urn:ex/supply-air-in> a ns1:InletConnectionPoint ;
    rdfs:label "supply air in" ;
    ns1:cnx <urn:ex/parallel-fan-powered-terminal-unit-constant-volume-fan>,
        <urn:ex/supply-air-in-connection> ;
    ns1:connectsThrough <urn:ex/supply-air-in-connection> ;
    ns1:hasMedium ns1:Fluid-Air ;
    ns1:hasProperty <urn:ex/supply-air-flow> ;
    ns1:isConnectionPointOf <urn:ex/parallel-fan-powered-terminal-unit-constant-volume-fan> .

<urn:ex/fan> a ns1:Fan,
        <http://data.ashrae.org/standard223/1.0/extensions/g36#Fan> ;
    rdfs:label "fan" ;
    ns1:cnx <urn:ex/fan-in>,
        <urn:ex/fan-out> ;
    ns1:hasConnectionPoint <urn:ex/fan-in>,
        <urn:ex/fan-out> ;
    ns1:hasProperty <urn:ex/fan-start-cmd>,
        <urn:ex/fan-start-status> .

<urn:ex/damper> a ns1:Damper,
        <http://data.ashrae.org/standard223/1.0/extensions/g36#Damper> ;
    rdfs:label "damper" ;
    ns1:cnx <urn:ex/damper-in>,
        <urn:ex/damper-out> ;
    ns1:hasConnectionPoint <urn:ex/damper-in>,
        <urn:ex/damper-out> ;
    ns1:hasProperty <urn:ex/damper-command>,
        <urn:ex/damper-position> .

<urn:ex/reheat-coil-valve> a ns1:Valve,
        <http://data.ashrae.org/standard223/1.0/extensions/g36#ChilledWaterValve>,
        <http://data.ashrae.org/standard223/1.0/extensions/g36#HotWaterValve> ;
    rdfs:label "reheat coil valve" ;
    ns1:cnx <urn:ex/reheat-coil-valve-in>,
        <urn:ex/reheat-coil-valve-out> ;
    ns1:hasConnectionPoint <urn:ex/reheat-coil-valve-in>,
        <urn:ex/reheat-coil-valve-out> ;
    ns1:hasMedium ns1:Water-HotWater ;
    ns1:hasProperty <urn:ex/reheat-coil-valve-command>,
        <urn:ex/reheat-coil-valve-position> .

<urn:ex/parallel-fan-powered-terminal-unit-constant-volume-fan> a ns1:FanPoweredTerminal,
        <http://data.ashrae.org/standard223/1.0/extensions/g36#FanPoweredTerminal> ;
    rdfs:label "parallel fan powered terminal unit constant volume fan" ;
    ns1:cnx <urn:ex/out>,
        <urn:ex/return-air-from-plenum-in>,
        <urn:ex/supply-air-in> ;
    ns1:contains <urn:ex/damper>,
        <urn:ex/fan>,
        <urn:ex/junction>,
        <urn:ex/reheat-coil>,
        <urn:ex/reheat-coil-valve> ;
    ns1:hasConnectionPoint <urn:ex/out>,
        <urn:ex/return-air-from-plenum-in>,
        <urn:ex/supply-air-in> .

<urn:ex/junction> a ns1:Junction ;
    rdfs:label "junction" ;
    ns1:cnx <urn:ex/junction-in-1>,
        <urn:ex/junction-in-2>,
        <urn:ex/junction-out> ;
    ns1:hasConnectionPoint <urn:ex/junction-in-1>,
        <urn:ex/junction-in-2>,
        <urn:ex/junction-out> ;
    ns1:hasMedium ns1:Fluid-Air .

<urn:ex/reheat-coil> a ns1:HeatingCoil,
        <http://data.ashrae.org/standard223/1.0/extensions/g36#HotWaterCoil> ;
    rdfs:label "reheat coil" ;
    ns1:cnx <urn:ex/reheat-coil-air-out>,
        <urn:ex/reheat-coil-in>,
        <urn:ex/reheat-coil-water-in>,
        <urn:ex/reheat-coil-water-out> ;
    ns1:hasConnectionPoint <urn:ex/reheat-coil-air-out>,
        <urn:ex/reheat-coil-in>,
        <urn:ex/reheat-coil-water-in>,
        <urn:ex/reheat-coil-water-out> ;
    ns1:hasRole ns1:Role-Heating .

